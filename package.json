    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "webpack": "4.2.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/glob": "^7.1.1",
    "@types/jquery": "^3.3.29",
    "@types/md5": "^2.1.33",
    "@types/mithril": "^1.1.15",
    "@types/mongodb": "^3.1.19",
    "@types/node": "^10.12.21",
    "@types/node-fetch": "^2.1.6",
    "@types/shortid": "^0.0.29",
    "@types/xregexp": "^3.0.29",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.0",
    "dotenv": "^6.2.0",
    "esm": "^3.2.0",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "image-hash": "^2.0.0",
    "imghash": "^0.0.7",
