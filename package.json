    "mithril": "^1.1.6",
    "mongodb": "^3.1.13",
    "node-fetch": "^2.3.0",
    "pre-commit": "^1.2.2",
    "shortid": "^2.2.14",
    "trash": "^4.3.0",
    "ts-node": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.1",
    "viewerjs": "^1.3.2",
    "xregexp": "^4.2.4"
  },
  "scripts": {
    "start": "ts-node -r esm src/server.ts",
    "generate": "ts-node -r esm src/script/generate.ts",
    "build": "webpack --config webpack.prod.js",
    "pre-commit": "yarn run build && git add .",
    "front:watch": "webpack --watch webpack.dev.js",
    "back:watch": "ts-node-dev -r esm src/server.ts",
    "watch": "yarn run back:watch",
    "dev": "concurrently 'yarn run front:watch' 'yarn run back:watch'"
  },
  "pre-commit": [
    "pre-commit"
  ]
}
